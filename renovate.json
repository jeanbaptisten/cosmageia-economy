{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": [
		"config:base"
	],
	"prHourlyLimit": 10,
	"schedule": [
		"before 5am on monday"
	],
	"packageRules": [
		{
			"matchPackagePatterns": [
				"maven"
			],
			"excludePackagePatterns": [
				"spring-boot"
			],
			"groupName": "Maven plugins"
		}
	]
}
